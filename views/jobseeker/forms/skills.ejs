<form id="skillForm">
    
     
       <div id="skillList">
           <h3>Skills</h3>
           <div>
               <button id="addSkill" type="button" class="btn btn-default">Add</button>
               <button  type="button" class="btn btn-default cancel">Cancel</button>
           </div>
           <ul>
               <li>Skill</li>
           </ul>
       </div>
     
       <div id="newSkill">
           <h3>New Skill</h3>
          <label class="control-label col-sm-4">Name</label>
          <div class="col-sm-8">
                <input type="text" class="form-control" id="skillName" name="skillName"  placeholder="Skill Name" >
          </div>
         <label class="control-label col-sm-4">Category</label>
          <div class="col-sm-8">
                <input type="text" class="form-control" id="skillCategory" name="skillCategory"  placeholder="Skill Category" >
          </div>
        
          <button id="saveSkill" class="save editSkill btn btn-default">Save</button>
          <button type="button" class="btn btn-default cancel">Cancel</button>
       </div>
  
    
   
  </form>