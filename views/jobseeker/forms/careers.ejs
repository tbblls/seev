<form id="careerForm">
    
     
       <div id="careerList">
           <h3>Career History</h3>
           <div>
               <button id="addCareer" type="button" class="btn btn-default">Add</button>
               <button  type="button" class="btn btn-default cancel">Cancel</button>
           </div>
           <ul>
               <li>Career</li>
           </ul>
       </div>
     
       <div id="newCareer">
           <h3>New Career</h3>
          <label class="control-label col-sm-4">Role</label>
          <div class="col-sm-8">
                <input type="text" class="form-control" id="careerRole" name="careerRole"  placeholder="Role" >
          </div>
          <label class="control-label col-sm-4">Company</label>
          <div class="col-sm-8">
                <input type="text" class="form-control" id="careerCompany" name="careerCompany"  placeholder="Company" >
          </div>
        
          <label class="control-label col-sm-4">Started</label>
          <div class="col-sm-8">
                <input type="month" class="form-control" id="careerStartmonth" name="careerStartmonth"  placeholder="Started" >
          </div>
        
          <label class="control-label col-sm-4">Ended</label>
          <div class="col-sm-8">
                <input type="month" class="form-control" id="careerEndmonth" name="careerEndmonth"  placeholder="Ended" >
          </div>
        
         <label class="control-label col-sm-4">Description</label>
          <div class="col-sm-8">
                <input type="text" class="form-control" id="careerDescription" name="careerDescription"  placeholder="Description" >
          </div>
        
        
          <button id="saveCareer" class="save editCareer btn btn-default">Save</button>
          <button type="button" class="btn btn-default cancel">Cancel</button>
       </div>
  
    
   
  </form>